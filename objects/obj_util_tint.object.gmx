            <string>var color, alpha, diff;

var cur_time = date_get_hour(global.time) * 60 + date_get_minute(global.time);

// Between 5 AM and 6:30 AM -- Night -&gt; Sunrise
if (300 &lt;= cur_time and cur_time &lt; 390)
{
    diff = (cur_time - 300) / 90;
    color = merge_color(color_night, color_sunrise, diff);
    alpha = lerp(alpha_night, alpha_sunrise, diff);
}

// Between 6:30 AM and 8 AM -- Sunrise -&gt; Day
else if (390 &lt;= cur_time and cur_time &lt; 480)
{
    diff = (cur_time - 390) / 90;
    color = merge_color(color_sunrise, color_day, diff);
    alpha = lerp(alpha_sunrise, alpha_day, diff);
}

// Between 8 AM and 5 PM -- Day
else if (480 &lt;= cur_time and cur_time &lt; 1020)
{
    color = color_day;
    alpha = alpha_day;
}

// Between 5 PM and 6:30 PM -- Day -&gt; Sunset
else if (1020 &lt;= cur_time and cur_time &lt; 1110)
{
    diff = (cur_time - 1020) / 90;
    color = merge_color(color_day, color_sunset, diff);
    alpha = lerp(alpha_day, alpha_sunset, diff);
}

// Between 6:30 PM and 8 PM -- Sunset -&gt; Night
else if (1110 &lt;= cur_time and cur_time &lt; 1200)
{
    diff = (cur_time - 1110) / 90;
    color = merge_color(color_sunset, color_night, diff);
    alpha = lerp(alpha_sunset, alpha_night, diff);
}

// Between 8 PM and 5 AM -- Night
else
{
    color = color_night;
    alpha = alpha_night;
}

draw_set_alpha(alpha);
draw_set_color(color);
draw_rectangle(view_xview[0], view_yview[0], view_xview[0] + view_wview[0], view_yview[0] + view_hview[0], false);

draw_set_alpha(1);
draw_set_color(c_white);
</string>

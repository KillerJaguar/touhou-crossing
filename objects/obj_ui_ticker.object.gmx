        <arguments>
          <argument>
            <kind>1</kind>
            <string>// update visibility

show = tick_count &gt; 0 or ds_queue_size(queue) &gt; 0;

var vis = show_rate;
if (not show)
    vis = -vis;

height = clamp(height + vis, 0, max_height);
visible = height != 0;

// update ticks

if (tick_count &gt; 0)
{
    for (var i = 0; i &lt; tick_count; i++)
        tick_x[|i] -= tick_speed;
    
    if (tick_x[|0] &lt; 0)
    {
        ds_list_delete(tick_str, 0);
        ds_list_delete(tick_x, 0);
        tick_count--;
    }
}

// add ticks

if (height == max_height)
{
    draw_set_font(font);
    
    while (ds_queue_size(queue) &gt; 0)
    {
        var str = ds_queue_dequeue(queue), xx;
        
        if (tick_count &gt; 0 and tick_x[|tick_count-1] &gt; 320 - buffer)
            xx = tick_x[|tick_count-1] + buffer + string_width(str);
        else
            xx = 320 + string_width(str);
        
        ds_list_add(tick_str, str);
        ds_list_add(tick_x, xx);
        tick_count++;
    }
}
</string>
          </argument>
        </arguments>
      </action>
    </event>
